<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<HBox fx:id="mainHBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="600.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dawidmotyka.cryptonose2.controllers.CryptonoseGuiAlertSettingsController">
   <children>
      <VBox HBox.hgrow="ALWAYS">
         <children>
            <TitledPane fx:id="p1periodSettingsTitledPane" animated="false" collapsible="false" VBox.vgrow="ALWAYS">
               <content>
                  <VBox spacing="7.0">
                     <children>
                        <HBox alignment="CENTER_LEFT" spacing="7.0" VBox.vgrow="NEVER">
                           <children>
                              <Label style="-fx-font-weight: bold;" text="Requiered % change" />
                              <Region HBox.hgrow="ALWAYS" />
                              <Label text="rising" />
                              <TextField fx:id="p1requiredRisingTextField" prefWidth="70.0" />
                              <Label text="dropping" />
                              <TextField fx:id="p1requiredDroppingTextField" prefWidth="70.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" spacing="7.0" VBox.vgrow="NEVER">
                           <children>
                              <Label style="-fx-font-weight: bold;" text="Requiered relative change" />
                              <Region HBox.hgrow="ALWAYS" />
                              <Label text="rising" />
                              <TextField fx:id="p1requiredRelativeRisingTextField" prefWidth="70.0" />
                              <Label text="dropping" />
                              <TextField fx:id="p1requiredRelativeDroppingTextField" prefWidth="70.0" />
                           </children>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </HBox>
                        <Separator prefWidth="200.0" />
                        <HBox alignment="CENTER_LEFT" spacing="7.0" VBox.vgrow="NEVER">
                           <children>
                              <Label style="-fx-font-weight: bold;" text="Sufficient relative change" />
                              <Region HBox.hgrow="ALWAYS" />
                              <Label text="rising" />
                              <TextField fx:id="p1sufficientRelativeRisingTextField" prefWidth="70.0" />
                              <Label text="dropping" />
                              <TextField fx:id="p1sufficientRelativeDroppingTextField" prefWidth="70.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" spacing="7.0" VBox.vgrow="NEVER" />
                     </children>
                     <padding>
                        <Insets bottom="7.0" left="7.0" right="7.0" top="7.0" />
                     </padding>
                  </VBox>
               </content>
               <graphic>
                  <Label fx:id="p1titleLabel" text="Title">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
               </graphic>
               <VBox.margin>
                  <Insets bottom="7.0" left="7.0" right="7.0" top="7.0" />
               </VBox.margin>
            </TitledPane>
            <Region VBox.vgrow="ALWAYS" />
            <TitledPane fx:id="p2periodSettingsTitledPane" animated="false" collapsible="false" VBox.vgrow="ALWAYS">
               <content>
                  <VBox spacing="7.0">
                     <children>
                        <HBox alignment="CENTER_LEFT" spacing="7.0" VBox.vgrow="NEVER">
                           <children>
                              <Label style="-fx-font-weight: bold;" text="Requiered % change" />
                              <Region HBox.hgrow="ALWAYS" />
                              <Label text="rising" />
                              <TextField fx:id="p2requiredRisingTextField" prefWidth="70.0" />
                              <Label text="dropping" />
                              <TextField fx:id="p2requiredDroppingTextField" prefWidth="70.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" spacing="7.0" VBox.vgrow="NEVER">
                           <children>
                              <Label style="-fx-font-weight: bold;" text="Requiered relative change">
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin></Label>
                              <Region HBox.hgrow="ALWAYS" />
                              <Label text="rising" />
                              <TextField fx:id="p2requiredRelativeRisingTextField" prefWidth="70.0" />
                              <Label text="dropping" />
                              <TextField fx:id="p2requiredRelativeDroppingTextField" prefWidth="70.0" />
                           </children>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </HBox>
                        <Separator prefWidth="200.0" />
                        <HBox alignment="CENTER_LEFT" spacing="7.0" VBox.vgrow="NEVER">
                           <children>
                              <Label style="-fx-font-weight: bold;" text="Sufficient relative change" />
                              <Region HBox.hgrow="ALWAYS" />
                              <Label text="rising" />
                              <TextField fx:id="p2sufficientRelativeRisingTextField" prefWidth="70.0" />
                              <Label text="dropping" />
                              <TextField fx:id="p2sufficientRelativeDroppingTextField" prefWidth="70.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" spacing="7.0" VBox.vgrow="NEVER" />
                     </children>
                     <padding>
                        <Insets bottom="7.0" left="7.0" right="7.0" top="7.0" />
                     </padding>
                  </VBox>
               </content>
               <graphic>
                  <Label fx:id="p2titleLabel" text="Title">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
               </graphic>
               <VBox.margin>
                  <Insets bottom="7.0" left="7.0" right="7.0" top="7.0" />
               </VBox.margin>
            </TitledPane>
            <HBox alignment="CENTER_RIGHT" spacing="7.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#cancelClick" prefWidth="100.0" text="Cancel" />
                  <Button minWidth="100.0" mnemonicParsing="false" onAction="#saveForAllExchangesClick" text="Save for all exchanges" />
                  <Button mnemonicParsing="false" onAction="#saveClick" prefWidth="100.0" text="Save" />
               </children>
               <VBox.margin>
                  <Insets bottom="7.0" left="7.0" right="7.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</HBox>
